//Введите два числа и вычислите их сумму,  произведение, разность и частное.
var a,b,s,p,r,ch:real;
begin
Writeln('введите 2 вещественнных числа');
readln(a,b);
s:=a+b;
p:=a*b;
r:=a-b;
ch:=a/b;
Writeln('сумма =',' ',s:0:3);
Writeln('произведение равно',' ',p:0:3);
Writeln('разность равна',' ',r:0:3);
Writeln('частное равно',' ',ch:0:3);
end.


