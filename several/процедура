procedure Swap(var a,b:real);
var x:real;
begin
x:=a;
a:=b;
b:=x;
end;
var a,b,c:real;
begin
writeln('Введите 3 действительных числа:');
readln(a,b,c);
if a>b then Swap(a,b);//сравниваем 1 и 2
if b>c then Swap(b,c);//сравниваем 2 и 3
if a>b then Swap(a,b);//снова 1 и 2
write(a:0:2,' ',b:0:2,' ',c:0:2);
end.
